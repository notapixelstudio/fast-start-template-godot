; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="Template Project"
config/icon="res://screens/logo_complete.png"

[autoload]

global="*res://globals/global.gd"
persistance="*res://globals/persistance.gd"

[display]

window/size/height=800

[editor_plugins]

enabled=PoolStringArray( "net.kivano.fsm", "vnen.tiled_importer" )

[gui]

theme/use_hidpi=true
theme/custom="res://assets/theme/theme.tres"

[input]

debug=[ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777238,"unicode":0,"echo":false,"script":null)
 ]

[rendering]

environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://default_env.tres"

[tiled_importer]

enable_json_format=true
